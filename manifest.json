{
  "manifest_version": 2,
  "name": "Mission Control",
  "version": "0.0.1",
  "description": "Get back control of your life by freeing yourself from digital addiction!",
  "options_page": "views/options/options.html",
  "author": "Zack Harley",
  "background": {
    "page": "views/background/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_popup": "views/popup/popup.html",
    "default_icon": {
      "16": "assets/images/logo16.png",
      "32": "assets/images/logo32.png",
      "64": "assets/images/logo64.png",
      "128": "assets/images/logo128.png"
    }
  },
  "icons": {
    "16": "assets/images/logo16.png",
    "32": "assets/images/logo32.png",
    "64": "assets/images/logo64.png",
    "128": "assets/images/logo128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "storage",
    "webNavigation",
    "tabs"
  ]
}
